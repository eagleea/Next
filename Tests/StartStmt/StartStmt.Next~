

item i1 {()}
item i2 {(string squeak = "squeak")}
item i3 {(int size = 50)}


character c1 {(int life = 10),()}
character c2 {(int life = 10),()}

int count = 10;

location l1 {(int sizex = 10000, int sizey=9283), (), ()}
location l2 {(),(),()}
location l2 {(),(),()}


start l1 end (count == 0 or c1.life == 0){
	output "in l1";
	choose (a,"1","1") (b,"2","2") (c,"3","3"){
		when a 
		{
			output "go to l1";
		} next l1
		when b
		{
			output "go to l2";
		} next l2
		when c
		{
			output "go to l3";
		} next l3
	}
}

start l2 end (count == 0 or c1.life == 0){
	output "in l2";
	choose (a,"1","1") (b,"2","2") (c,"3","3"){
		when a 
		{
			output "go to l1";
		} next l1
		when b
		{
			output "go to l2";
		} next l2
		when c
		{
			output "go to l3";
		} next l3
	}
	
}

start l3 end (count == 0 or c1.life == 0) {
	output "in l3";
	choose (a,"1","1") (b,"2","2") (c,"3","3"){
		when a 
		{
			output "go to l1";
		} next l1
		when b
		{
			output "go to l2";
